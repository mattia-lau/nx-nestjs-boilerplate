{
  "name": "nx-nestjs-starter",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "mattia lau"
  },
  "repository": {
    "url": "https://github.com/mattia-lau/nx-nestjs-boilerplate",
    "type": "git"
  },
  "keywords": [
    "monorepo",
    "nx",
    "nx nestjs"
  ],
  "scripts": {
    "start": "nx run-many --target=serve --all",
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "test-e2e": "nx run-many --target=test-e2e --all",
    "prepare": "husky install",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/platform-express": "8.4.4",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.4",
    "@nrwl/cli": "14.2.2",
    "@nrwl/eslint-plugin-nx": "14.2.2",
    "@nrwl/jest": "14.2.2",
    "@nrwl/linter": "14.2.2",
    "@nrwl/node": "14.2.2",
    "@nrwl/workspace": "14.2.2",
    "@types/jest": "27.5.2",
    "@types/node": "16.11.39",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "lint-staged": "13.0.1",
    "nx": "14.2.2",
    "prettier": "2.6.2",
    "supertest": "6.2.3",
    "ts-jest": "27.1.5",
    "typescript": "4.7.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,yml}": [
      "prettier --write "
    ],
    "**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
}
