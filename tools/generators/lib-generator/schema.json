{
  "cli": "nx",
  "id": "app-generator",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "App name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "importPath": {
      "type": "string",
      "description": "The library name used to import it, like `@myorg/my-awesome-lib`. Must be a valid npm name."
    }
  },
  "required": ["name"]
}
